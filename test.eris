
fib :: (n: i32) : i32 {
    num1 := 0;
    num2 := 1;
    num3 := 1;
    i := 0;
    while i < n {
        i += 1;
        num3 = num1 + num2;
        num1 = num2;
        num2 = num3;
    }
    while 1 == 1 {
        return num1;
    }
    return 0;
}


main :: () : i32 {
    f := fib(40);
    printf("%d\n", f);
    return 0;
}
